<odoo>

  <!-- Category Import Module Management -->
  <record model="ir.module.category" id="category_product_approval">
    <field name="name">Product Approval</field>
    <field name="description">Enable to Approve Product</field>
    <field name="sequence">10</field>
  </record>

  <record id="group_product_approval" model="res.groups">
    <field name="name">Product Manager</field>
    <field name="category_id" ref="category_product_approval" />
    <field name="users" eval="[(4, ref('base.user_root'))]" />
  </record>
</odoo>